  "version": "4.0.0",
  "author": "Daniel Lewis",
  "dependencies": {
    "gatsby": "^3.13.1",
    "gatsby-link": "^3.13.0",
    "gatsby-plugin-image": "^1.14.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-postcss": "^4.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-typography": "^3.13.0",
    "gatsby-react-router-scroll": "^4.13.0",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-transformer-json": "^3.13.0",
    "gatsby-transformer-sharp": "^3.13.0",
    "prop-types": "15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-scroll-parallax": "^2.4.0",
    "react-typist": "^2.0.5",
    "react-typography": "^0.16.19",
    "react-wavify": "^1.3.0",
    "typography": "^0.16.19",
    "typography-theme-judah": "^0.16.19"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js gatsby-starter-tailwind",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js gatsby-starter-tailwind --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "^10.3.4",
    "babel-eslint": "10.1.0",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.25.2",
    "gatsby-plugin-eslint": "^4.0.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
